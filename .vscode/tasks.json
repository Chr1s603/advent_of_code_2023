{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "mix",
			"task": "Build",
			"problemMatcher": [
				"$mixCompileError",
				"$mixCompileWarning"
			],
			"group": "build",
			"label": "mix: Build"
		},
		{
			"type": "mix",
			"task": "Format",
			"group": "none",
			"label": "mix: Format"
		},
		{
			"type": "mix",
			"task": "Run",
			"group": "none",
			"label": "mix: Run",
			"problemMatcher": [
				"$mixCompileError"
			]
		}
	]
}